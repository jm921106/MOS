<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sejune.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splash" ></activity>
        <activity android:name=".newAccount"></activity>
        <activity android:name=".forgetPW"></activity>
        <activity android:name=".selectStore"></activity>
        <activity android:name=".newStore"></activity>
        <activity android:name=".Base"></activity>
        <activity android:name=".newSchedule"></activity>
        <activity android:name=".newNotice"></activity>
        <activity android:name=".newMessage"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>